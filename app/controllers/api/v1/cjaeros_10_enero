rails generate scaffold Report id:integer atm_id:integer type:integer reported_at:timestamp

curl "localhost:3000/reports" -d "report[atm_id]=498&report[type]=1"


curl -XPOST -H "Content-Type: application/json" "localhost:3000/reports" -d '
{
  "report": {
    "atm_id": "600",
    "type_id": "1",
    "reported_at": '2008-08-20 16:56:21'
  }
}'


CREATE TABLE reports (
 id serial PRIMARY KEY,
 atm_id integer NOT NULL,
 type integer NOT NULL,
 reported_at timestamp DEFAULT current_timestamp
);


curl -v -XPOST -H "Accept: application/json" -H "Content-Type: application/json" "localhost:3000/reports" -d '
{
  "report": {
    "atm_id": "900",
    "type_id": "1",
    "reported_at": "2013-01-31"
  }
}'

rails generate scaffold Atm branch:string municipality_id:integer street:string district:string zipcode:string latitude:decimal longitude:decimal bank_id:integer --skip-migration

rails generate scaffold Bank name:string

rails generate scaffold Municipality name:string state_id:integer latitude:decimal longitude:decimal

rails generate scaffold State name:string code:string latitude:decimal longitude:decimal zoom:decimal space:integer

http://0.0.0.0:3000/api/v1/atms/zipcode?zipcode=62577&radius=1
http://0.0.0.0:3000/api/v1/atms/nearby?latitude=18.921298&longitude=-99.174111&radius=5&bank=1